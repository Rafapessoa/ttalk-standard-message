            "$ref": "#/definitions/Sequencial"
          }
        }
      }
    },
    "Sequencial": {
      "type": "object",
      "properties": {
        "tc_id": {
          "type": "string",
          "example": 1,
          "description": "Identificador do relacionamento",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "tc_id",
              "Required": true,
              "type": "int",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Lote": {
          "type": "string",
          "example": 201200000076,
          "description": "Número do lote",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "lote_id",
              "Required": true,
              "type": "char",
              "length": 12,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Registro de log": {
          "type": "string",
          "example": 20,
          "description": "Registro de log",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "log_registro",
              "Required": true,
              "type": "int",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Log de operação": {
          "type": "string",
          "example": "I",
          "description": "Tipo de operação, Inserção, Alteração ou Exclusão",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "log_operacao",
