  "definitions": {
    "RegistroSequencial": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/Paging"
        },
        {
          "$ref": "#/definitions/ListOfSequencial"
        },
        {
          "$ref": "#/definitions/ListOfCollumns"
        }
      ]
