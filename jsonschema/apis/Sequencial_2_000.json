          "type": "string",
          "example": 1,
          "description": "Sequencial da alteração",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "rel_lote_sequencial",
              "Required": true,
              "type": "int",
              "available": true,
              "canUpdate": false
            }
