              "Required": true,
              "type": "char",
              "length": 15,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Log do programa": {
          "type": "string",
          "example": "Aduaneiro",
          "description": "Programa da alteração",
          "x-totvs": [
            {
              "product": "sara",
              "Field": "log_programa",
              "Required": true,
              "type": "varchar",
              "length": 70,
              "available": true,
              "canUpdate": false
            }
          ]
        },
